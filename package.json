{
  "name": "bookmarks_history",
  "version": "0.1.0",
  "description": "Chrome extension which keeps track of all the changes you made to your bookmarks",
  "scripts": {
    "start": "NODE_ENV=development webpack --watch",
    "build": "NODE_ENV=production webpack",
    "tslint": "tslint --project tsconfig.json",
    "eslint": "eslint *.js tools/**/*.js src/**/*.js",
    "sass-lint": "sass-lint 'src/**/*.scss' -v -q",
    "lint": "npm run tslint && npm run eslint && npm run sass-lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladimir-tikhonov/bookmarks_history.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "bookmarks",
    "history"
  ],
  "author": "Vladimir Tikhonov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vladimir-tikhonov/bookmarks_history/issues"
  },
  "homepage": "https://github.com/vladimir-tikhonov/bookmarks_history#readme",
  "devDependencies": {
    "@types/chrome": "0.0.69",
    "@types/react": "^16.3.18",
    "@types/react-dom": "^16.0.6",
    "@types/uuid": "^3.4.3",
    "babel-minify-webpack-plugin": "^0.3.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "file-loader": "^1.1.11",
    "generate-json-webpack-plugin": "^0.3.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.3",
    "ts-loader": "^4.4.1",
    "tslib": "^1.9.2",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-chrome-extension-reloader": "^0.8.3",
    "webpack-cli": "^3.0.7",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "chrome-promise": "^3.0.2",
